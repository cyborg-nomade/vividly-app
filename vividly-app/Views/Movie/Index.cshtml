@model vividly_app.ViewModels.MovieIndexViewModel
@{
    ViewBag.Title = "AllMoviesView";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Movies</h2>


<table class="table table-hover table-bordered">
    <thead>
        <tr>
            <th>
                Movies
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var movie in @Model.Movies)
        {
            <tr>
                <td>@movie.Name</td>
            </tr>
        }
    </tbody>
</table>

